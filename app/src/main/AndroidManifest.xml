<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codeLearningApp.coding">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/coding_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coding_app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.codeLearningApp.coding.ui.code.RunCodeActivity"
            android:label="Compiler">
        </activity>
        <activity
            android:name="com.codeLearningApp.coding.ui.login.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3341551705792260~6664621013" />

        <activity
            android:name="com.codeLearningApp.coding.ui.subQuestion.SubQuestionActivity"
            android:label="Questions" />
        <activity
            android:name="com.codeLearningApp.coding.ui.objQuestion.ObjQuizActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.codeLearningApp.coding.ui.code.ProblemActivity"
            android:label="Program" />
        <activity
            android:name="com.codeLearningApp.coding.ui.code.CodeActivity"
            android:label="Code" />
        <activity
            android:name="com.codeLearningApp.coding.ui.code.CategoryActivity"
            android:label="Choose Category" />
        <activity
            android:name="com.codeLearningApp.coding.ui.main.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.codeLearningApp.coding.ui.WebViewActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>